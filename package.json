{
  "name": "node-typescript-template-eslint",
  "version": "1.0.0",
  "description": "template typescript nodejs with es-lint",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": true,
  "engines": {
    "node": ">=14.15.3",
    "npm": ">=6.14.11"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/{repository}.git"
  },
  "bugs": {
    "url": "https://github.com/{repository}/issues"
  },
  "contributors": [
    "stephen.deletang@shopopop.com"
  ],
  "scripts": {
    "start": "ts-node src/index.ts",
    "test": "jest --reporters=default",
    "build": "tsc --build",
    "version": "npm run build",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "typecheck": "tsc --noEmit",
    "release": "standard-version",
    "prerelease": "standard-version -- --prerelease",
    "fix": "standard-version -- --release-as minor",
    "doc": "rimraf docs && npx typedoc src/index.ts"
  },
  "keywords": [
    "eslint",
    "nodejs",
    "typescript",
    "es6",
    "browser"
  ],
  "author": "tech-shopopop <tech@shopopop.com>",
  "license": "ISC",
  "simple-git-hooks": {
    "pre-commit": "npx commitlint --edit $1 & npm run lint",
    "pre-push": "npm run typecheck"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@stephen-shopopop/eslint-config": "^1.4.5",
    "@stephen-shopopop/ts-config": "^1.1.7",
    "@types/node": "^17.0.23",
    "eslint": "8.8.0",
    "eslint-plugin-jest": "^26.1.3",
    "jest": "^27.5.1",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.7.0",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.1.4",
    "typedoc": "^0.22.13"
  },
  "dependencies": {
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
